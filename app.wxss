.page-result {
    z-index: -1;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    display: flex;
    align-items: center;
    justify-content: center;
}

.panel-result {
    padding: 300rpx 0;
}

.page-result-content {
    margin-top: -100rpx;
}

.page-result .center {
    width: 100%;
    text-align: center;
    opacity: 0.25;
}
.page-result .icon {
    font-size: 93rpx;
    padding-bottom: 10rpx;
}